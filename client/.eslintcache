   </p>\n          <form\n            className=\"flex-row justify-center justify-space-between-md align-center\"\n            onSubmit={handleFormSubmit}\n          >\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"replyText\"\n                placeholder=\"Add your response...\"\n                value={responseText}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n                Send\n              </button>\n            </div>\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to send your messages. Please login{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default ResponseForm;\n","C:\\Users\\19568\\Desktop\\GROUPPROJECT3\\CrockettPrinting\\client\\src\\pages\\Products\\Wrapper\\index.js",[],"C:\\Users\\19568\\Desktop\\GROUPPROJECT3\\CrockettPrinting\\client\\src\\pages\\Products\\ProductCard\\index.js",[],"C:\\Users\\19568\\Desktop\\GROUPPROJECT3\\CrockettPrinting\\client\\src\\pages\\LogIn\\index.js",[],"C:\\Users\\19568\\Desktop\\GROUPPROJECT3\\CrockettPrinting\\client\\src\\pages\\SingleMessage\\index.js",[],"C:\\Users\\19568\\Desktop\\GROUPPROJECT3\\CrockettPrinting\\client\\src\\pages\\SignUp\\index.js",[],"C:\\Users\\19568\\Desktop\\GROUPPROJECT3\\CrockettPrinting\\client\\src\\components\\Header\\index.js",[],"C:\\Users\\19568\\Desktop\\GROUPPROJECT3\\CrockettPrinting\\client\\src\\components\\SingleCategory\\index.js",["104","105"],"import React, { useState } from \"react\";\nimport ProductOptionsCard from \"./ProductOptionsCard\";\nimport Wrapper from \"./Wrapper\";\nimport { Q